    "express": "^4.16.1",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.0",
    "morgan": "~1.9.1",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.3",
    "mocha": "^9.2.0",
    "node-mocks-http": "^1.11.0",
